@model IEnumerable<Business.Entites.Journal>

@{
    ViewBag.Title = "Index";
}

<h2>Subscribed Journals</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.JournalName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th></th>
    </tr>

    
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.JournalName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.ActionLink("Details", "Details", new { id = item.JournalId })
            </td>
        </tr>
    }

</table>
